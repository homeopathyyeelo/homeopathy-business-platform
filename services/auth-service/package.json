{
  "name": "@yeelo/auth-service",
  "version": "1.0.0",
  "description": "Authentication microservice for Yeelo Homeopathy Platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "docker:build": "docker build -t yeelo/auth-service .",
    "docker:run": "docker run -p 3001:3001 yeelo/auth-service"
  },
  "dependencies": {
    "express": "^4.18.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "jose": "^5.8.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/node": "^20.10.5",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": ["auth", "microservice", "jwt", "homeopathy"],
  "author": "Yeelo Homeopathy",
  "license": "MIT"
}
