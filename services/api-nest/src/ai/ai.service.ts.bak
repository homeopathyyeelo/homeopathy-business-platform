import { Injectable } from "@nestjs/common"

@Injectable()
export class AIService {
  // Content generation
  async generateContent(request: any) {
    return { message: "AI disabled", request }
  }

  // Embeddings
  async generateEmbeddings(request: { texts: string[]; model?: string; requestor_user_id?: string }) {
    return { embeddings: request.texts?.map(() => Array(8).fill(0)), model: request.model || "disabled" }
  }

  // Models listing
  async getModels() {
    return { models: [] }
  }

  // Prompt templates
  async getPrompts(key?: string, language?: string) {
    return { prompts: [], key, language }
  }

  // Requests listing
  async getRequests(requestorUserId?: string, modelId?: string, page = 1, limit = 20) {
    return { requests: [], pagination: { page, limit, total: 0, pages: 0 }, filters: { requestorUserId, modelId } }
  }

  // Single request
  async getRequest(id: string) {
    return { id, status: "not_found" }
  }

  // Forecast demand
  async forecastDemand(request: { product_id: string; shop_id: string; days_ahead?: number; requestor_user_id?: string }) {
    return { product_id: request.product_id, shop_id: request.shop_id, forecast: [], days_ahead: request.days_ahead || 7 }
  }

  // Dynamic pricing
  async calculateDynamicPricing(request: { product_id: string; current_price: number; current_stock: number; expiry_date?: string; demand_forecast?: number; cost_price: number; requestor_user_id?: string }) {
    return { product_id: request.product_id, suggested_price: request.current_price, reason: "AI disabled" }
  }

  // Campaign content
  async generateCampaignContent(request: { campaign_id: string; type: string; target_audience: any; requestor_user_id?: string }) {
    return { campaign_id: request.campaign_id, content: "", notes: "AI disabled" }
  }

  // Product recommendations
  async generateProductRecommendations(request: { customer_id: string; order_items: any[]; requestor_user_id?: string }) {
    return { customer_id: request.customer_id, recommendations: [] }
  }

  // Usage analytics
  async getUsageAnalytics(startDate?: string, endDate?: string, modelId?: string) {
    return { startDate, endDate, modelId, usage: [] }
  }
}
