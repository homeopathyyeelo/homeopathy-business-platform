{
  "name": "homeopathy-business-platform",
  "version": "0.2.0",
  "private": true,
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "services/*",
    "packages/*",
    "app"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "npm run test:e2e",
    "start": "turbo run start",
    "dev:app": "next dev",
    "build:app": "next build",
    "start:app": "next start",
    "db:generate": "npm --workspace @yeelo/shared-db run generate",
    "db:migrate": "npm --workspace @yeelo/shared-db run migrate",
    "db:migrate:deploy": "npm --workspace @yeelo/shared-db run migrate:deploy",
    "db:seed": "npm --workspace @yeelo/shared-db run seed",
    "test:e2e": "playwright test --reporter=list",
    "test:e2e:headed": "playwright test --headed"
  },
